  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@hooks/*": ["hooks/*"],
      "@service/*": ["service/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"],
      "@models/*": ["models/*"],
      "@components/*": ["components/*"]
    }
  }
}
